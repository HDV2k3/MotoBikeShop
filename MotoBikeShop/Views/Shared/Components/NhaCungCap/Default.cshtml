
<div class="col-lg-12">
    <div class="mb-3">
        <h4>Hãng Xe</h4>
        <ul class="list-unstyled fruite-categorie">
            @foreach (var nhacc in Model)
            {
                <li>
                    <div class="d-flex justify-content-between fruite-name">
                        <a asp-controller="hangHoa" asp-action="Index" as asp-route-loai="@nhacc.MaNCC">

                            <i class="fas  me-2"></i>@nhacc.TenCongTy
                        </a>
                      
                    </div>
                </li>
            }
        </ul>
    </div>
</div>
