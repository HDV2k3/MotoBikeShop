        <script>
        // Khi trang được tải hoàn thành
        $(document).ready(function () {
            // Thiết lập carousel
            $('.carousel').carousel({
                interval: 2000, // Tự động chuyển đổi mỗi 1 giây
           
            });

            // Cập nhật vị trí phân trang khi carousel chuyển đổi ảnh
            $('.carousel').on('slide.bs.carousel', function () {
                var currentIndex = $(this).find('.carousel-item.active').index() + 1;
                $('.carousel-indicators li').removeClass('active');
                $('.carousel-indicators li:nth-child(' + currentIndex + ')').addClass('active');
            });
        });
</script>
    <script>
        // Khi trang được tải hoàn thành
        $(document).ready(function () {
            // Thiết lập carousel
            $('.slide-show-hoat-dong').slick({
                slidesToShow: 1,
                slidesToScroll: 1,
            });

            // Xử lý sự kiện khi nhấp vào biểu tượng "next"
            $('#product-next').click(function () {
                $('.slide-show-hoat-dong').slick('slickNext');
            })

            // Xử lý sự kiện khi nhấp vào biểu tượng "prev"
            $('#product-back').click(function () {
                $('.slide-show-hoat-dong').slick('slickPrev');
            })

            // Xử lý sự kiện khi nhấp vào các phần tử trong carousel
            $('.homepage-operation-slide-tab').click(function (e) {
                e.preventDefault();
                var targetSlide = $(this).data('slick-index');
                $('.slide-show-hoat-dong').slick('slickGoTo', targetSlide);
            });
        });
    </script>
</body>

